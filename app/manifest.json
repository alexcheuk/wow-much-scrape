{
    "name": "WoW, Much Scrape",
    "version": "0.6.1",
    "manifest_version": 2,
    "description": "WoW, Much Scrape",
    "icons": {
        "16": "images/icon-16.png",
        "128": "images/icon-128.png"
    },
    "default_locale": "en",
    "background": {
        "scripts": [
            "scripts/crxreload_client.js",
            "scripts/background.js"
        ]
    },
    "browser_action": {
        "default_icon": {
            "19": "images/icon-19.png",
            "38": "images/icon-38.png",
            "128": "images/icon-128.png"
        }
    },
    "options_page": "options.html",
    "update_url": "http://alexcheuk.com/armory/update/updates.xml",
    "permissions": [
        "tabs",
        "notifications",
        "storage",
        "http://*/*",
        "https://*/*"
    ],
    "web_accessible_resources": [
        "scripts/*"
    ]
}